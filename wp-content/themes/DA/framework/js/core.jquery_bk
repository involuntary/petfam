function toggleMarkers(e, catid){
 
	if ( jQuery(e).is(':checked') ){
	visible = true;
	} else {
	visible = false;
	}

    jQuery(AllMarkers).each(function(id, marker) {
	 
		if(marker.catid == catid || catid == 0 ){
